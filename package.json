  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simoneferrero/sf-minis-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run -b chrome --headless",
    "deps:update": "ncu -u",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint \"src\"",
    "serve": "gatsby serve",
    "serve:cy": "yarn build && yarn serve -p 7070",
    "serve:cy:ci": "yarn serve -p 7070",
    "start": "yarn develop",
    "start:cy": "yarn develop -p 7070",
    "test": "start-server-and-test start:cy http://localhost:7070 cy:open",
    "test:cy": "start-server-and-test serve:cy http://localhost:7070 cy:run",
    "test:cy:ci": "start-server-and-test serve:cy:ci http://localhost:7070 cy:run",
    "validate": "yarn format && yarn lint"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "yarn validate"
  }
}
